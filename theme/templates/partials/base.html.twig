<!DOCTYPE html>
<html lang="en">
<head>
	{% block head %}
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">


	<title>{% if header.title %}{{ header.title|e('html') }} | {% endif %}{{ site.title|e('html') }}</title>
	
	{% if header.description %}
		<meta name="description" content="{{ header.description }}">
	{% else %}
		<meta name="description" content="{{ site.description }}">
	{% endif %}
	
	<meta name="keywords" content="dogs, agility, dog training, eskie, american eskimo, dog agility, diy, home renovation" />

	
	<link rel="icon" href="{{ url('theme://images/favicon.ico') }}" />
	<!--
	<link rel="canonical" href="{{ page.url(true, true) }}" />
	-->
	
	<link href="{{ theme_url }}/css/style.css" type="text/css" rel="stylesheet" />

	{% endblock head %}
</head>

<body>
{% include 'partials/header.html.twig' %}

{% block content %}
{% endblock %}

<!--
