<script src="{{ theme_url }}/bower_components/jquery/dist/jquery.js"></script>
<script src="{{ theme_url }}/bower_components/foundation/js/foundation/foundation.js"></script>
<script src="{{ theme_url }}/bower_components/foundation/js/foundation/foundation.clearing.js"></script>
<script src="{{ theme_url }}/bower_components/foundation/js/foundation/foundation.interchange.js"></script>
<script src="{{ theme_url }}/bower_components/foundation/js/foundation/foundation.topbar.js"></script>
